<table>
  <th>When / Quando </th>
  <th>Map's link </th>
  <th>Where / Onde </th>
  <th>Contact / Contato </th>

  <% for(let show of shows) { %>
   <tr>
    <td>
      <%- show.when %>
    </td>
    <td>
      <%=show.mapsLink%>
    </td>
    <td>
      <%=show.address%>
    </td>
    <td>
      <%=show.contact%>
    </td>
  </tr> 
  <% } %>
  
</table>